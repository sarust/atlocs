%section.main-content
  .container
    .row
      %p#notice= notice
      %p
        %strong Name
        = @collection.name
      %p
        %strong Description:
        = @collection.description
      %p
        %strong Collection preffix:
        = @collection.prefix
      %p
        = image_tag @collection.image.url(:medium)
        = link_to 'Edit', edit_collection_path(@collection)
